{
    "name": "sirgrimorum/cms",
    "description": "Simple cms and translation utility from Database an JS",
    "keywords": ["laravel", "javascript", "localization", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andres Espinosa",
            "email": "andres.espinosa@grimorum.com"
        }
    ],
    "require": {
        "php": ">=5.6.19",
        "laravel/framework": "5.*",
        "illuminate/support": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.6"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/extensions",
            "src/models"
        ],
        "psr-0": {
            "Sirgrimorum\\Cms\\": "src/",
            "Sirgrimorum\\Cms\\Models": "app/models/"
        }
    },
    "minimum-stability": "stable"
}

